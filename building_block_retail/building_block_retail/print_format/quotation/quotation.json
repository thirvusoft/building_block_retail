 "html": "<table>\n    <tr>\n        <td colspan=6 class=\"text-center\">\n            <u>QUOTATION</u>\n             {% if letter_head %}\n              {{ letter_head }}\n             {% endif %}\n        </td>\n    </tr>\n    <tr>\n        <td colspan=3 style= \"border-right:0px;\">\n            <b>{{ _(\"Quotation No:\") }}\n            {{ doc.name }}</b> \n        </td>\n        <td colspan=3 class=\"text-right\">\n            <b>{{ _(\"Date:\") }}\n             {{ doc.get_formatted(\"transaction_date\") }}</b>\n        </td>\n    </tr>\n    <tr>\n        <td style =\"width:20px;border-right:0px;\">\n            <b>{{ _(\"To:\") }}</b>\n        </td>\n        <td colspan=5 style=\"width:200px;height:70px;\">\n            {{ doc.customer_address or\"\" }}<br>\n            {{ doc.address_display or\"\" }}<br>\n            \n        </td>\n    </tr>\n\n    <tr>\n        <td style= \"border-right:0px;\"></td>\n        <td class=\"text-left\" style= \"border-right:0px;\">\n            <b>{{ _(\"GSTIN / UIN :\") }} \n            {{ doc.customer_gstin or\"\" }}</b>\n        </td>\n        <td style= \"border-right:0px;\"></td>\n        <td style= \"border-right:0px;\"></td>\n        <td style= \"border-right:0px;\"></td>\n        <!--<td class=\"text-center\">-->\n        <!--    <b>{{ _(\"Contact :\") }} -->\n        <!--    {{ doc.contact_mobile }}</b>-->\n        <!--</td>-->\n        <td></td>\n    </tr>\n    </table>\n    <table style=\"border-top:none;\">\n    <thead>\n    <tr style=\"border-top:none;\">\n        <td style =\"width:20px;\"><b>{{ _(\"SI.NO\") }}</b></td>\n        <td class=\"text-center\"><b>{{ _(\"Paver Type\") }}</b></td>\n        <td class=\"text-center\" style=\"width:80px\"><b>{{ _(\"Qty\") }}</b></td>\n        <td class=\"text-center\" style=\"width:80px\"><b>{{ _(\"Unit\") }}</b></td>\n        <td class=\"text-center\" style=\"width:80px\"><b>{{ _(\"Price\") }}</b></td>\n        <td class=\"text-center\" style=\"width:90px\"><b>{{ _(\"Amount\") }}</b></td>\n    </tr>\n    </thead>\n    <tbody>\n     {% if doc.items|length < 20 %}\n        {%- for item in range(19) -%} \n           {% if item < doc.items|length %}\n            <tr>\n              <td>{{ doc.items[item].idx }}</td>\n              <td>{{ doc.items[item].item_name or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].qty or\"\" }}</td>\n              <td>{{ doc.items[item].stock_uom or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].rate  or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].amount or\"\" }}</td>\n            </tr>\n           {% endif %} \n          {% if item >= doc.items|length %}\n            <tr style=\"border-bottom:none;\">\n              <td></td>\n              <td></td>\n              <td></td>\n              <td></td>\n              <td></td>\n              <td></td>\n            </tr>\n          {% endif %} \n       {% endfor %}\n     {% endif %}   \n     <tr>\n      {% if doc.items|length >20 %}\n         {%- for item in range(doc.items|length) -%} \n              <td>{{ doc.items[item].idx }}</td>\n              <td>{{ doc.items[item].item_name or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].qty or\"\" }}</td>\n              <td>{{ doc.items[item].stock_uom or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].rate  or\"\" }}</td>\n              <td class=\"text-right\">{{ doc.items[item].amount or\"\" }}</td>\n        {% endfor %}\n      {% endif %}   \n       </tr>\n        </tbody>\n    <!--<tfoot>-->\n    <!--    <tr style=\"border-top:none;\">-->\n    <!--        <td></td>-->\n    <!--        <td></td>-->\n    <!--        <td></td>-->\n    <!--        <td></td>-->\n    <!--        <td></td>-->\n    <!--        <td></td>-->\n    <!--    </tr>-->\n    <!--</tfoot>    -->\n    \n        </table>\n        \n        <table style=\"page-break-inside: avoid;\">\n    <tr style=\"height:120px; border-top:1px solid black;\">\n        <td>\n            <b>{{ _(\"Terms & Conditions:\") }}</b><br>\n            {{ doc.terms or\"\" }}\n        </td>\n    </tr>\n    </table>\n    <table style=\"page-break-inside: avoid; border-top:none;border-top:1px solid black\">\n    <tr>\n        <td colspan=3 style=\"page-break-inside: avoid; border-top:none\"><b>{{ _(\"Remarks:\") }}<br>\n        Extra GST 18%</b>\n        </td>\n    </tr>\n    <tr style=\"border-bottom:0px; border-right:0px;\">\n        <td style= \"border-right:0px;\"></td>\n        <td style= \"border-right:0px;\"></td>\n        <td class=\"text-right\"><b>{{ _(\"for SHRI VELAVAA PAVER BRICKS\") }}</b></td>\n    </tr>\n    <tr style=\"border-top:0px;  height:70px;\">\n        <td class=\"text-left\" style= \"border-right:0px;\"><br><br><br><b>{{ _(\"CLIENT SIGNATURE\") }}</b>\n        <td class=\"text-center\" style=\"width:250px;border-right:0px;\"><br><br><br><b>{{ _(\"THANK YOU\") }}</td>\n        <td class=\"text-right\" style= \"border-right:0px;\"><br><br><br><b>{{ _(\"AUTHORIZED SIGNATURE\") }}</b></td>\n    </tr>\n</table>",
