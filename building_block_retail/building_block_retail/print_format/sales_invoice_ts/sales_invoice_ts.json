0%\">\n            <td>\n                Invoice No :<b> {{doc.name}}</b>\n            </td>\n            <td style=\"width:50%\">\n                Dated :<b> {{doc.get_formatted(\"posting_date\")}} </b>\n            </td>\n        </tr>\n        <tr>\n            <td>\n             {% if doc.customer_address %}\n                        \n                        {% set var1 = frappe.get_doc(\"Address\",doc.customer_address) %}\n                        {% set var3 = var1.gstin or \" \"%}\n                        {% set vara = var1.address_title or '' %}\n                        {% set varb = var1.address_line1 or '' %}\n                        {% set varc = var1.address_line2 or '' %}\n                         {% set vard = var1.city or '' %}\n                        {% set vare = var1.pincode or '' %}\n                        \n                        {% endif %}\n                        \n                        {% if doc.shipping_address_name %}\n                        \n                        {% set var2 = frappe.get_doc(\"Address\",doc.shipping_address_name) %}\n                        {% set var4 = var2.gstin or '' %}\n                        {% set var5 = var2.city or ' ' %}\n                        {% set varp = var2.address_title or '' %}\n                        {% set varo = var2.address_line1 or '' %}\n                        {% set vari = var2.address_line2 or '' %}\n                         \n                        {% set varu = var2.pincode or ' '%}\n                         {% endif %}\n                       <table > \n                       <tr style=\"font-siz:12px;height:125px;\">\n                        <td style =\"width:25%;\"> <b> Billed to :</b> </td>\n                       <td style =\"width:75%;\"><b> {{doc.customer_name}}<br>{% if vara %}{{ vara or ' '}}<br>   {% endif %}</b>{% if varb %} {{ varb or ' '}}<br>{% endif %} {% if varc %} {{varc or ' '}}<br>{% endif %} \n                             {% if vard %}{{ vard or ' '}}<br>{% endif %} {% if vare %} {{vare or ' '}}{% endif %} <br></td>\n                            </tr>\n                       <tr>\n                            <td><b> GSTIN /UIN :</b></td>\n                        <td> {{ var3 or \" \" }} </td></tr>\n                       </table>\n                </td>\n                <td>\n                    <table>\n                <tr style=\"padding:3px;font-siz:12px;height:125px;\"> \n                    <td style =\"width:25%;\"><b> Shipped to :</b></td>\n                    <td style =\"width:75%;\"><b> {{ varp or ' '}}</b><br> {{ varo or ' '}}<br> {{vari or ' '}}<br> {{ var5 or ' '}}<br> {{varu or ' '}}<br></td></tr>\n                    <tr> <td> <b> GSTIN /UIN :</b> </td><td> {{ var4 or \" \"}} </td></tr> \n            </table>\n                </td>\n        </tr>\n    </table>\n   \n     </div>   \n \n \n    <table class=\"master1\" style =\"width:100%;height:220px;\">\n      <thead>\n        <tr style=\"text-align:center;border-bottom:1px solid black;border-left:1px solid;height:10px;\">\n            <td style=\"width:4%;border-right:1px solid black;width:5px\">SI No</td>\n            <td style=\"width:28%;border-right:1px solid black;width:55%\">Description of Goods</td>\n            <td style=\"width:14%;border-right:1px solid black;text-align:center;width:10%\">HSN/SAC</td>\n            \n            <td style=\"width:10%;border-right:1px solid black;text-align:center;\">Quantity</td>\n            <td style=\"width:10%;border-right:1px solid black;text-align:center;\">Unit</td>\n            \n            <td style=\"width:14%;border-right:1px solid black;text-align:center;\">Price</td>\n        \n            <td style=\"width:16%;border-right:1px solid black;\">Amount</td>\n        </tr>\n        </thead>\n        {% set serial_no=[] %}\n        {% for item in doc.items %}\n        {% set _=serial_no.append(1) %}\n            <tr style=\"height:20px\">\n                <td style=\"text-align:center;border-right:1px solid black;border-left:1px solid;\">{{ serial_no|length }}</td>\n                <td style=\"border-right:1px solid black;\"><b>{{ item.description or \" \" }}</td>\n                <td style=\"border-right:1px solid black;text-align:right;\">{{item.gst_hsn_code or \" \"}}</td>\n                \n              \n                 <td style=\"text-align:right;border-right:1px solid black;text-align:right;\">{{item.qty}}</td>\n               \n                <td style=\"text-align:right;border-right:1px solid black;text-align:right;\"> {{item.uom}}</td>\n                <td style=\"border-right:1px solid black;text-align:right;border-right:1px solid black;\">{{item.rate}}</td>\n      \n              <td style=\"text-align:right;border-right:1px solid;\"><b>{{item.amount}}</b></td>\n            </tr>\n        {% endfor %}\n        {% for empty in range(10-doc.items|length) %}\n                <tr style=\"height:20px;\">\n                    <td style=\"border-right:1px solid black;border-left:1px solid;\"></td>\n                    <td style=\"border-right:1px solid black;\"></td>\n                   \n                    <td style=\"border-right:1px solid black;\"></td>\n                    <td style=\"border-right:1px solid black;\"></td>\n                    <td style=\"border-right:1px solid black;\"></td>\n                    <td style=\"border-right:1px solid black;\"></td>\n                   <td style=\"border-right:1px solid black;\"></td>\n                   \n                </tr>\n        {% endfor %}\n        <tfoot>\n        <tr style=\"border-top:1px solid;border-bottom:1px solid black;height:10px\">\n            <td style=\"border-left:1px solid\"></td>\n            <td style=\"border-right:1px solid black;text-align:right;\">Total</td>\n            <td style=\"border-right:1px solid black;\"></td>\n          \n            <td style=\"border-right:1px solid black;text-align:right;\">{{doc.total_qty}}</td>\n            <td style=\"border-right:1px solid black;\"></td>\n            <td style=\"border-right:1px solid black;text-align:right;\"> </td>\n            <td style=\"text-align:right;border-right:1px solid;\"> {{doc.total}} </td>\n        </tr>\n        </tfoot>\n    </table>\n    <table class=\"master1\" style =\"width:100%;\">\n        <tr>\n            <td style= \"border-left:1px solid;border-right:1px solid;\"> Amount Chargeable (in words):<b>{{ frappe.utils.money_in_words(doc.total) }}</b></td>\n           \n        </tr>\n    </table>\n    {% if doc.gst_category==\"SEZ\" %}\n    <table   class=\"master1\" style =\"width:100%;height:30px;\">\n        <tr>\n            <td style=\"width:50%;text-align:center;border-right:1px solid black;border-top:0.5px solid black;border-left:1px solid;\">HSN/SAC</td>\n            <td style=\"text-align:center;border-top:0.5px solid black;border-right:1px solid black;\">Taxable</td>\n            <td colspan=\"2\" style=\"border-top:0.5px solid black;border-right:1px solid black;text-align:center;\">Integrated Tax</td>\n            <td style=\"border-top:0.5px solid black;border-right:1px solid black;text-align:center;\">Total </td>\n        </tr>\n        <tr>\n            <td style=\"text-align:center;border-right:1px solid black;border-left:1px solid;\"></td>\n            <td style=\"text-align:center;border-right:1px solid black;\">Value</td>\n            <td style=\"border-right:1px solid black;border-top:1px solid black;text-align:center;\">Rate</td>\n            <td style=\"border-right:1px solid black;border-top:1px solid black;text-align:center;\">Amount</td>\n            <td style=\"border-right:1px solid black;text-align:center;\">Tax Amount </td>\n        </tr>\n        {% set total = [] %}\n        {% for i in taxcal %}\n        {% set _ = total.append(taxcal[i]) %}\n        <tr style=\"text-align:center;border-top:1px solid black;\">\n            <td style=\"width:50%;border-right:1px solid black;border-top:1px solid black;border-left:1px solid;\">{{i}}</td>\n    